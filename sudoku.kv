WelcomePage:
<WelcomePage>:
    orientation: "vertical"
    Label:
        text: ''
    Label:
        text: ''
    Label:
        text:  '[color=f05a31] [b] [u] Sudoku [/u] [/b] [/color]'
        font_name: 'Roboto'
        markup: True
        font_size: '40sp'
        color: 	1, 10, 1, 1
    Label:
        text: ''
    Label:
        orientation: "horizontal"
        font_name: 'Roboto'
        size_hint_x: 1
        text: '[color=a2f42a] Please Select the Difficulty Level  [/color]'
        markup: True
        font_size: '18sp'
    Button:
        font_name: 'Roboto'
        text: "[b] Easy [/b] "
        background_color: 0.1, 0.5, 0.6, 1
        spacing: 6
        thickness: 3
        margin: 2
        markup: True
        pos_hint:{'center_x':.5, 'center_y':.5}
        size_hint:(.30, .70)
        height: "25dp"
        on_press: app.root.selected_level(1)
    Button:
        font_name: 'Roboto'
        text: "[b] Medium [/b] "
        background_color: 0.1, 0.5, 0.6, 1
        markup: True
        pos_hint:{'center_x':.5, 'center_y':.5}
        size_hint:(.30, .70)
        height: "25dp"
        on_press: app.root.selected_level(2)
    Button:
        text: "[b] Tricky [/b] "
        background_color: 0.1, 0.5, 0.6, 1
        markup: True
        pos_hint:{'center_x':.5, 'center_y':.5}
        size_hint:(.30, .70)
        height: "25dp"
        on_press: app.root.selected_level(3)
    Label:
        text: ''
    Button:
        text: "Quit"
        background_color: 2,12,12,0.2
        on_press: app.stop()
        pos_hint:{'center_x':.5, 'center_y':.5}
        size_hint:(.30, 1)
    Label:
        text: ''
    Label:
        text: ''

